digraph {

	rankdir=LR

//	splines=line
	splines=ortho

	node[fillcolor=black label="" style=filled width=0.25]

	startNode[shape=circle]

	simpleNode[
		fillcolor=green
		label=simple
		shape=rectangle
		style="rounded, filled"
	]

	endNode[shape=doublecircle ]

	startNode -> simpleNode -> endNode
}